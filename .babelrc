{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-syntax-dynamic-import"],
  "env": {
    "server": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    }
  }
}
